<Window x:Class="MigrationApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MigrationApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="700">
    <Grid>
        <TextBox x:Name="tbxSiteId" HorizontalAlignment="Left" Height="23" Margin="140,64,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblSiteId" Content="SiteId" HorizontalAlignment="Left" Margin="140,38,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnLoad" Content="Load" HorizontalAlignment="Left" Margin="310,65,0,0" VerticalAlignment="Top" Width="75" Click="btnLoad_Click"/>
        <Button x:Name="btnExecute" Content="Execute" HorizontalAlignment="Left" Margin="310,90,0,0" VerticalAlignment="Top" Width="75" Click="btnExecute_Click"/>
        <Label x:Name="lblStatusBar" Content="Label" HorizontalAlignment="Left" Margin="10,433,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.579,0.423" Width="672"/>
        <ComboBox x:Name="cbxSiteIds" HorizontalAlignment="Left" Margin="10,64,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cbxSiteIds_SelectionChanged"/>
        <Label x:Name="lblSiteSelect" Content="Select Site Id" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
